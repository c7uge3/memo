{
  "version": 2,
  "builds": [
    {
      "src": "api/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/sw.js",
      "dest": "/sw.js",
      "headers": { "cache-control": "public, max-age=0, must-revalidate" }
    },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
